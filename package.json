{
  "name": "@ikamva/ngx-tailwindcss",
  "version": "0.0.2",
  "description": "Tailwind CSS angular schematic",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:clean:launch": "yarn build && yarn clean:launch",
    "clean": "git checkout HEAD -- sandbox && git clean -f -d sandbox",
    "clean:launch": "yarn clean && yarn launch",
    "dev": "yarn build:clean:launch",
    "sandbox:ng-add": "cd sandbox && ng g ngx-tailwindcss-schematic:ng-add",
    "link:schematic": "yarn link && cd sandbox && yarn link @ikamva/ngx-tailwindcss",
    "publish": "yarn build",
    "test": "yarn build:clean:launch && yarn test:sandbox && yarn clean",
    "update:sandbox": "cd sandbox && ng update --all"
  },
  "keywords": [
    "schematics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Applicafroguy/ngx-tailwindcss-schematic"
  },
  "author": "Sivuyile Magutywa",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.0.2",
    "@angular-devkit/schematics": "^9.0.2",
    "@angular/cdk": "^9.0.0",
    "@schematics/angular": "^9.0.2",
    "typescript": "~3.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0"
  }
}
